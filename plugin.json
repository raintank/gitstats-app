{
  "type": "app",
  "name": "Github Stats",
  "id": "raintank-gitstats-app",
  "routes": [
    {
      "path": "graphite/*",
      "method": "*",
      "url": "https://tsdb.raintank.io/graphite",
      "reqSignedIn": true,
      "reqRole": "Viewer",
      "headers": [
        { "name": "Authorization", "content": "Bearer {{.SecureJsonData.apiKey}}"}
      ]
    },
    {
      "path": "elasticsearch/*",
      "method": "*",
      "url": "https://tsdb.raintank.io/elasticsearch",
      "reqSignedIn": true,
      "reqRole": "Viewer",
      "headers": [
        { "name": "Authorization", "content": "Bearer {{.SecureJsonData.apiKey}}"}
      ]
    },
    {
      "path": "api/*",
      "method": "*",
      "url": "https://task-server.raintank.io/api/v1/",
      "reqSignedIn": true,
      "reqRole": "Admin",
      "headers": [
        { "name": "Authorization", "content": "Bearer {{.SecureJsonData.apiKey}}"}
      ]
    }
  ],

  "info": {
    "description": "Github Stats",
    "author": {
      "name": "Raintank.",
      "url": "http://raintank.io"
    },
    "keywords": ["raintnak", "Github"],
    "logos": {
      "small": "img/logo_small.png",
      "large": "img/logo_large.png"
    },
    "links": [
      {"name": "Project site", "url": "http://raintank.io"},
      {"name": "License & Terms", "url": "http://raintank.io"}
    ],
    "screenshots": [],
    "version": "1.0.1",
    "updated": "2016-05-25"
  },

  "includes": [
    {
      "type": "dashboard",
      "name": "Github Stats",
      "path": "dashboards/github-stats.json",
      "addToNav": true,
      "defaultNav": true
    }
  ],
  "dependencies": {
    "grafanaVersion": "3.0+",
    "plugins": []
  }
}
